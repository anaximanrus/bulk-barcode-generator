{
  "name": "barcode-generator",
  "version": "1.0.0",
  "private": true,
  "description": "Bulk barcode generator with configurable limits",
  "scripts": {
    "dev": "pnpm --parallel --filter \"./frontend\" --filter \"./backend\" dev",
    "build": "pnpm --filter \"./shared\" build && pnpm --filter \"./frontend\" --filter \"./backend\" build",
    "lint": "pnpm --recursive lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "pnpm --recursive type-check"
  },
  "keywords": [
    "barcode",
    "qr-code",
    "code128",
    "bulk-generator",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
